                              <p>Author(s): {authors}</p>\n                                </div>\n                            </div>\n                            <div className='mt-3'>{description}</div>\n                        </div>\n                        <div className='modal-footer'>\n                            <div className='left-slide'>\n                                <a href={previewLink} className='btn-link' color='default' type='button' target='_blank' rel='noopener noreferre'>Preview Book</a>\n                            </div>\n                            <div className='divider'></div>\n                            <div className='right-slide'>\n                                <a href={infoLink} className='btn-link' color='default' type='button' target='_blank' rel='nooopener noreferre'>Book Info</a>\n                            </div>\n                        </div>\n                    </Modal>\n                </Card>\n            )\n}\n\nexport default BookData;","/Users/felipecruz/Documents/Projects/React/BookNotes/book-notes-client/src/components/Search.js",[],"/Users/felipecruz/Documents/Projects/React/BookNotes/book-notes-client/src/reducers/BooksReducer.js",[],"/Users/felipecruz/Documents/Projects/React/BookNotes/book-notes-client/src/serviceWorker.js",[],{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","severity":1,"message":"81","line":18,"column":17,"nodeType":"82","endLine":18,"endColumn":88},{"ruleId":"76","replacedBy":"83"},{"ruleId":"78","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":10,"column":5,"nodeType":"87","messageId":"88","endLine":12,"endColumn":6},{"ruleId":"89","severity":1,"message":"90","line":39,"column":26,"nodeType":"91","messageId":"92","endLine":39,"endColumn":28},{"ruleId":"93","severity":1,"message":"94","line":79,"column":33,"nodeType":"82","endLine":79,"endColumn":45},{"ruleId":"76","replacedBy":"95"},{"ruleId":"78","replacedBy":"96"},{"ruleId":"97","sever